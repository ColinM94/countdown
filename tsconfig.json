{
  "compilerOptions": {
    "allowSyntheticDefaultImports": false,
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "api*": [
        "src/api*"
      ],
      "components*": [
        "src/components*"
      ],
      "common*": [
        "src/common*"
      ],
      "contexts*": [
        "src/contexts*"
      ],
      "library*": [
        "src/library*"
      ],
      "navigation*": [
        "src/navigation*"
      ],
      "screens*": [
        "src/screens*"
      ],
    },
  },
}